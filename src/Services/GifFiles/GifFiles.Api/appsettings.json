{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": { "path": "./logs/log-.txt", "rollingInterval": "Day" }
      }
    ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Gifs": "User ID=postgres;Password=admin;Host=localhost;Port=5433;Database=poppersdb;"
  }
}
